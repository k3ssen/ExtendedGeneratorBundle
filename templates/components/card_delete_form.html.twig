{% extends 'components/card_form.twig' %}

{% block body %}
    <p>Are you sure you want to delete this entry?</p>
{% endblock %}

{% block card_form_buttons %}
    <button class="btn btn-danger">{{ btn_text|default('Yes, delete') }}</button>
    <a href="#" onclick="window.history.back();" class="btn btn-info">{{ btn_text|default('No, go back') }}</a>
{% endblock %}