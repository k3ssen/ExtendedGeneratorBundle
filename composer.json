    "keywords": ["generator", "crud","entity", "entities", "meta", "metadata", "symfony", "symfony4"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Driessen",
            "email": "kevin3ssen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "k3ssen/generator": "dev-master"
    },
    "suggest": {
        "symfony/security-bundle": "~4.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "sg/datatablesbundle": "^1.0.4"
    },
    "autoload": {
        "psr-4": { "K3ssen\\ExtendedGeneratorBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
